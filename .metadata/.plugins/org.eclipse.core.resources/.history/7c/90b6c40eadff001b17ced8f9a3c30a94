package lamdaexercice;

import java.util.Collections;
import java.util.Comparator;
import java.util.List;

public class Fruits {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		List<String> fruits = List.of("Orange","Pineapple","Pear","Apple","Grapefruit","Peach",
				"Pear","Apprico","Pineapple");
		Collections.sort(fruits);
		//.stream().peek(e -> System.out.println(e));
		//fruits.stream().soter(e -> System.out.println(e));
		//fruits.stream().sorted( e -> forEach(e));
		fruits.stream().forEach(e -> System.out.println(e));
		//fruits.stream().distinct().forEach(e -> System.out.println(e));
		//fruits.stream().sorted(Comparator.reverseOrder()).distinct().forEach(e -> System.out.println(e));
		fruits.stream().sorted().distinct().forEach(e -> System.out.println(e));
		//fruits.stream().sorted(Comparator.reverseOrder()).distinct().forEach(e -> System.out.println(e));

		

	}

}
