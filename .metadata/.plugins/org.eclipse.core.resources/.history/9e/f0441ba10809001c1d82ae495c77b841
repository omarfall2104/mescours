package com.saraya;

public class Exercice4 {
	private Integer number;
	
	public Exercice4(Integer number){
		this.number = number;
	}
	
	public Integer getNumber() {
		return number;
	}

	public void setNumber(Integer number) {
		this.number = number;
	}

	@Override
	public String toString() {
		return "umber=" + number;
	}

	
	}

