package exercie.exercice;

import java.util.ArrayList;
import java.util.List;
import java.util.stream.Collectors;

public class NumberEven1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//ArrayList<Integer> number = ArrayList<Integer>();
		List<Integer> number = List.of(10,15,17);
		//number.add(12);
		//number.add(12);
		
		//System.out.println(number);
		number.stream().sorted().filter(e -> e%2==0).collect(Collectors.toList());
		//number.stream().filter(e -> e%2==0).map(e -> e*e).sorted().collect(Collectors.toList());
	}

}
