import java.awt.List;
import java.util.ArrayList;

public class Fruits {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		ArrayList<String> list = new ArrayList();
		list.add("Aple");
		list.add("Orange");
		list.add("Banana");
		for(String l : list) {
			System.out.println(l);
		}
		

	}

}
