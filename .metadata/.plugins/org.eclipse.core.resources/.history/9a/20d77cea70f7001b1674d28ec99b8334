
public class Enployes {
	private String nom;
	private String prenom; 
	private Phone phone;
	private Adresse adresse;
	
	public Employes(String nom, String prenom) {
		this.nom = nom;
		this.prenom =prenom; 
	}
	public void setPrenom(String prenom) {
		this.prenom = prenom;
		
	}
	public void setnom(String nom) {
		this.prenom = nom;
	}
	public String toString() {
		return prenom+ " "+ nom+ " ";
	}
	
}
